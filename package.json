  "scripts": {
    "build": "rollup -c",
    "checkHusky": "node checkIfHuskyExists.ts",
    "ci:cypress:storybook": "start-storybook -p 9009 -s public --ci",
    "ci:cypress:test:storybook:chrome": "cypress run --headless -C cypress.ci.json --browser chrome",
    "ci:cypress:test:storybook:chromium": "cypress run --headless -C cypress.ci.json --browser chromium",
    "ci:cypress:test:storybook:firefox": "cypress run --headless -C cypress.ci.json --browser firefox",
    "ci:storybook:chrome": "yarn start-server-and-test ci:cypress:storybook http://localhost:9009 ci:cypress:test:storybook:chrome",
    "ci:storybook:chromium": "yarn start-server-and-test ci:cypress:storybook http://localhost:9009 ci:cypress:test:storybook:chromium",
    "ci:storybook:firefox": "yarn start-server-and-test ci:cypress:storybook http://localhost:9009 ci:cypress:test:storybook:firefox",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --headless -C cypress.json",
    "husky:setup": "husky install && npx husky add .husky/pre-commit \"yarn prettier:fix\" && npx husky add .husky/pre-commit \"yarn lint\" && npx husky add .husky/pre-push \"yarn ci:storybook:chrome\" && npx husky add .husky/pre-push \"yarn ci:storybook:firefox\" && npx husky add .husky/pre-push \"yarn build\"",
    "lint": "eslint . --ext .tsx",
    "lint:fix": "eslint --fix",
    "prettier": "prettier --check **/*.{tsx,ts,js}",
    "prettier:fix": "prettier --write **/*.{tsx,ts,js}",
    "storybook": "yarn checkHusky && start-storybook -p 6006 -s public"
  }
